#ProgressBar {
    align-items: center;
    justify-content: flex-end;
    width: 50px;
    height: 100%;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    background-color: rgb(0, 0, 0);
}

#ProgressBar > #Background {
    flex-grow: 1;
    justify-content: flex-end;
    width: 100%;
    background-color: rgb(106, 18, 18);
}

#ProgressBar > #Background > #Percent {
    width: 100%;
    height: 0;
}
